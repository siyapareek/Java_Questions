class GfG
{
	boolean isBinary(String str)
	{
	  boolean flag = true;
	  int n = str.length();
	  char ch;
	  
	  for(int i = 0; i < n; i++){
	      ch = str.charAt(i);
	      if(ch == '1' || ch == '0'){
	          flag = true;
	      }
	      else{
	          flag = false;
	          break;
	      }
	  }
	  return flag;
	}
}
